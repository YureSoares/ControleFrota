        android:theme="@style/Theme.ControleFrota">
        <activity
            android:name=".model.TelaCadastro"
            android:label="@string/title_activity_tela_cadastro"
            android:theme="@style/Theme.ControleFrota.NoActionBar"></activity>
        <activity
            android:name=".TelaLogin"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResumoViagem" />
        <activity android:name=".HistoricoViagens" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.ControleFrota" />
    </application>

</manifest>